{
  "name": "mishka",
  "version": "15.0.0",
  "private": true,
  "description": "Личный проект «Мишка» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.1.x",
    "browser-sync": "^2.26.3",
    "gulp": "4.0.x",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "8.0.x",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "pixel-glass": "^1.0.13",
    "stylelint": "^9.10.1",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp css",
    "start": "gulp start"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.12"
  },
  "dependencies": {
    "gulp-concat": "^2.6.1",
    "normalize.css": "^8.0.1",
    "picturefill": "^3.0.3",
    "svgxuse": "^1.2.6"
  }
}
